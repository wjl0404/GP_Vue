C,IAArB;;AACA,QAAA,MAAI,CAAC,WAAL;AACD,OAJH,EAKG,KALH,CAKS,UAAC,CAAD,EAAO;AACZ,QAAA,OAAO,CAAC,GAAR,CAAY,CAAZ;AACD,OAPH;AAQD,KAhCM;AAkCP,IAAA,WAlCO,yBAkCO;AAAA;;AACZ,MAAA,eAAe,CAAC,WAAhB,CAA4B,KAAK,KAAjC,EACG,IADH,CACQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAtB;;AACA,QAAA,MAAI,CAAC,aAAL,CAAmB,IAAnB;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,QAAQ,CAAC,IAArB;AACD,OALH,EAMG,KANH,CAMS,UAAC,CAAD,EAAO;AACZ,QAAA,OAAO,CAAC,GAAR,CAAY,CAAZ;AACD,OARH;AASD;AA5CM,GAVI;AAwDb,EAAA,OAxDa,qBAwDH;AACR,SAAK,aAAL;AACD;AA1DY,CAAf","sourcesContent":["<template>\r\n  <div class=\"list row\">\r\n    <div class=\"col-md-8\">\r\n      <div class=\"input-group mb-3\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          placeholder=\"搜索待办清单\"\r\n          v-model=\"title\"\r\n        />\r\n        <div class=\"input-group-append\">\r\n          <button\r\n            class=\"btn btn-outline-secondary\"\r\n            type=\"button\"\r\n            @click=\"searchTitle\"\r\n          >\r\n            Search\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <h4>待办清单</h4>\r\n      <ul class=\"list-group\">\r\n        <li\r\n          class=\"list-group-item\"\r\n          :class=\"{ active: index == currentIndex }\"\r\n          v-for=\"(todo, index) in todos\"\r\n          :key=\"index\"\r\n          @click=\"setActiveTodo(todo, index)\"\r\n        >\r\n          {{ todo.title }}\r\n        </li>\r\n      </ul>\r\n\r\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllTodos\">\r\n        删除所有清单\r\n      </button>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <div v-if=\"currentTodo\">\r\n        <h4>待办详情</h4>\r\n        <div>\r\n          <label><strong>任务:</strong></label> {{ currentTodo.title }}\r\n        </div>\r\n        <div>\r\n          <label><strong>详情:</strong></label> {{ currentTodo.description }}\r\n        </div>\r\n        <div>\r\n          <label><strong>状态:</strong></label>\r\n          {{ currentTodo.done ? \"done\" : \"undone\" }}\r\n        </div>\r\n\r\n        <router-link :to=\"'/todo/' + currentTodo._id\" class=\"badge badge-warning\">编辑</router-link>\r\n        <!-- <router-link\r\n          :to=\"{\r\n            path: '/todo',\r\n          }\"\r\n        ></router-link> -->\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>点击右侧列表查看详情</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TodoDataService from \"../services/TodoDataService\";\r\n\r\nexport default {\r\n  name: \"todos-list\",\r\n  data() {\r\n    return {\r\n      todos: [],\r\n      currentTodo: null,\r\n      currentIndex: -1,\r\n      title: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    retrieveTodos() {\r\n      TodoDataService.getAll()\r\n        .then((response) => {\r\n          this.todos = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    refreshList() {\r\n      this.retrieveTodos();\r\n      this.currentTodo = null;\r\n      this.currentIndex = -1;\r\n    },\r\n\r\n    setActiveTodo(todo, index) {\r\n      this.currentTodo = todo;\r\n      this.currentIndex = todo ? index : -1;\r\n    },\r\n\r\n    removeAllTodos() {\r\n      TodoDataService.deleteAll()\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.refreshList();\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    searchTitle() {\r\n      TodoDataService.findByTitle(this.title)\r\n        .then((response) => {\r\n          this.todos = response.data;\r\n          this.setActiveTodo(null);\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.retrieveTodos();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}