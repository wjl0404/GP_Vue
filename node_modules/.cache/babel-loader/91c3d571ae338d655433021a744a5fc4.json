O,CAAC,GAAR,CAAY,QAAQ,CAAC,IAArB;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,IAAjB,GAAwB,MAAxB;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,SAAf;AACD,OALH,EAMG,KANH,CAMS,UAAA,CAAA,EAAK;AACV,QAAA,OAAO,CAAC,GAAR,CAAY,CAAZ;AACD,OARH;AASD,KA3BM;AA6BP,IAAA,UA7BO,wBA6BM;AAAA;;AACX,UAAI,IAAG,GAAI;AACT;AACA,QAAA,KAAK,EAAE,KAAK,WAAL,CAAiB,KAFf;AAGT,QAAA,WAAW,EAAE,KAAK,WAAL,CAAiB,WAHrB;AAIT,QAAA,MAAM,EAAE;AAJC,OAAX;AAMA,MAAA,eAAe,CAAC,MAAhB,CAAuB,KAAK,WAAL,CAAiB,GAAxC,EAA6C,IAA7C,EACG,IADH,CACQ,UAAA,QAAO,EAAK;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,QAAQ,CAAC,IAArB;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,QAAf;AACD,OAJH,EAKG,KALH,CAKS,UAAA,CAAA,EAAK;AACV,QAAA,OAAO,CAAC,GAAR,CAAY,CAAZ;AACD,OAPH;AAQD,KA5CM;AA8CP,IAAA,UA9CO,wBA8CM;AAAA;;AACX,MAAA,eAAe,CAAC,MAAhB,CAAuB,KAAK,WAAL,CAAiB,GAAxC,EACG,IADH,CACQ,UAAA,QAAO,EAAK;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,QAAQ,CAAC,IAArB;;AACA,QAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAE,UAAA,IAAI,EAAC;AAAP,SAAlB;AACD,OAJH,EAKG,KALH,CAKS,UAAA,CAAA,EAAK;AACV,QAAA,OAAO,CAAC,GAAR,CAAY,CAAZ;AACD,OAPH;AAQF;AAvDO,GARI;AAiEb,EAAA,OAjEa,qBAiEH;AACR,SAAK,OAAL,GAAe,EAAf;AACA,SAAK,OAAL,CAAa,KAAK,MAAL,CAAY,MAAZ,CAAmB,EAAhC;AACA,IAAA,OAAO,CAAC,GAAR,CAAY,KAAK,MAAL,CAAY,MAAZ,CAAmB,EAA/B;AACF;AArEa,CAAf","sourcesContent":["<template>\r\n  <div v-if=\"currentTodo\" class=\"edit-form\">\r\n    <h4>待办事宜</h4>\r\n    <form>\r\n      <div class=\"form-group\">\r\n        <label for=\"title\">任务：</label>\r\n        <input type=\"text\" class=\"form-control\" id=\"title\"\r\n          v-model=\"currentTodo.title\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"description\">详情：</label>\r\n        <input type=\"text\" class=\"form-control\" id=\"description\"\r\n          v-model=\"currentTodo.description\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label><strong>状态:</strong></label>\r\n        {{ currentTodo.done ? \"done\" : \"undone\" }}\r\n      </div>\r\n    </form>\r\n\r\n    <button class=\"badge badge-primary mr-2\"\r\n      v-if=\"currentTodo.done\"\r\n      @click=\"updatedone(false)\"\r\n    >\r\n      未完成\r\n    </button>\r\n    <button v-else class=\"badge badge-primary mr-2\"\r\n      @click=\"updatedone(true)\"\r\n    >\r\n      完成\r\n    </button>\r\n\r\n    <button class=\"badge badge-danger mr-2\"\r\n      @click=\"deleteTodo\"\r\n    >\r\n      删除\r\n    </button>\r\n\r\n    <button type=\"submit\" class=\"badge badge-success\"\r\n      @click=\"updateTodo\"\r\n    >\r\n      更新\r\n    </button>\r\n    <p>{{ message }}</p>\r\n  </div>\r\n\r\n  <div v-else>\r\n    <br />\r\n    <p>Please click on a Todo...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TodoDataService from \"../services/TodoDataService\";\r\n\r\nexport default {\r\n  name: \"todo\",\r\n  data() {\r\n    return {\r\n      currentTodo: null,\r\n      message: ''\r\n    };\r\n  },\r\n  methods: {\r\n    getTodo(id) {\r\n      console.log(id);\r\n      TodoDataService.get(id)\r\n        .then(response => {\r\n          this.currentTodo = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    updatedone(status) {\r\n      var data = {\r\n        // _id: this.currentTodo._id,\r\n        status: status\r\n      };\r\n      TodoDataService.update(this.currentTodo._id, data)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.currentTodo.done = status;\r\n          this.message = '状态修改成功。';\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    updateTodo() {\r\n      var data = {\r\n        // _id: this.currentTodo._id,\r\n        title: this.currentTodo.title,\r\n        description: this.currentTodo.description,\r\n        status: status\r\n      };\r\n      TodoDataService.update(this.currentTodo._id, data)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.message = '信息更新成功';\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    deleteTodo() {\r\n      TodoDataService.delete(this.currentTodo._id)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.$router.push({ path:'/todo' });\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.message = '';\r\n    this.getTodo(this.$route.params.id);\r\n    console.log(this.$route.params.id);\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.edit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}